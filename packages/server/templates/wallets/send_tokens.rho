new deployerId(`rho:rchain:deployerId`),
    deployId(`rho:rchain:deployId`),
    rl(`rho:registry:lookup`),
    walletCh
in {
    rl!({{ env_uri }}, *walletCh) |
    for(@(_, wallet) <- walletCh) {
        @wallet!(
            "sendTransfer",
            {{ timestamp }},
            {{ wallet_address_from }},
            {{ wallet_address_to }},
            {{ amount }},
            {{ description }}
        )
    }
}
