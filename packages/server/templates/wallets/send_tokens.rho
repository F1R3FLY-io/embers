new deployerId(`rho:rchain:deployerId`),
    deployId(`rho:rchain:deployId`),
    rl(`rho:registry:lookup`),
    walletsCh
in {
    rl!({{ env_uri }}, *walletsCh) |
    for(@(_, wallets) <- walletsCh) {
        @wallets!(
            "sendTransfer",
            {{ timestamp }},
            {{ wallet_address_from }},
            {{ wallet_address_to }},
            {{ amount }},
            {{ description }}
        )
    }
}
