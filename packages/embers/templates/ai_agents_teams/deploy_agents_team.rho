{% extends "common/insert_signed.rho" %}

{%- block name -%} agentsTeam {%- endblock -%}

{%- block initialization -%}
{%- filter indent(8) -%}

new devNull(`rho:io:devNull`),
{%- if !system_channels.is_empty() %}
    {{ system_channels | join(",\n") | indent(4) }},
{%- endif %}
    agentTeam
in
{
    contract agentsTeam(
        @input,
{%- if output %}
        return
{%- endif %}
    ) = {
{%- if !output_channels.is_empty() %}
        new {{ output_channels | join(",\n") | indent(12) }}
        in {
            {{ nodes | join(" |\n") | indent(12) }}
        }
{%- else %}
        {{ nodes | join(" |\n") | indent(8) }}
{%- endif %}
    }
}

{%- endfilter -%}
{%- endblock -%}
